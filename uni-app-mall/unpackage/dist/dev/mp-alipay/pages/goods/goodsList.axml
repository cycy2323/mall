<view><view class="header"><block a:for="{{filterByList}}" a:for-item="target" a:for-index="i" a:key="goods_id"><view data-event-opts="{{[['tap',[['handleTarget',[i]]]]]}}" class="{{(('target')+' '+((target.selected)?'on':''))}}" onTap="__e">{{''+target.text+''}}</view></block></view><view class="place"></view><view class="goods-list"><view class="product-list"><block a:for="{{goodsList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['handleGoods',['$0'],[[['goodsList','',index]]]]]]]}}" class="product" onTap="__e"><image src="{{item.img}}" mode="widthFix"></image><view class="name">{{item.name}}</view><view class="info"><view class="price">{{"￥"+item.price}}</view><view class="slogan">{{item.slogan+''}}<text>人付款</text></view></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>