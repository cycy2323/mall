<view><video hidden="{{!(showVideo)}}" class="myVideo" id="myVideo" src="{{videoSrc}}" data-event-opts="{{[['fullscreenchange',[['videoControl',['$event']]]]]}}" onFullscreenchange="__e"></video><view class="content"><view class="label"><block a:for="{{labelList}}" a:for-item="label" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['loadRatings',[index]]]]]}}" class="{{((index==labelIndex)?'on':'')}}" onTap="__e">{{''+label.name+"("+label.number+')'}}</view></block></view><view class="list"><block a:for="{{ratingsList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="ratings"><view class="left"><view class="face"><image src="{{item.face}}"></image></view></view><view class="right"><view class="name-date"><view class="username">{{item.username}}</view><view class="date">{{item.date}}</view></view><view class="spec">{{item.spec}}</view><view class="medias"><view class="content">{{item.content}}</view><view class="img-video"><block a:for="{{item.video}}" a:for-item="video" a:for-index="__i0__" a:key="path"><view data-event-opts="{{[['tap',[['playVideo',['$0'],[[['ratingsList','',index],['video','path',video.path,'path']]]]]]]}}" class="box" onTap="__e"><image mode="aspectFill" src="{{video.img}}"></image><view class="playbtn"><view class="icon iconfont">îŸ©</view></view></view></block><block a:for="{{item.img}}" a:for-item="image" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['showBigImg',['$0','$1'],[[['ratingsList','',index],['img','',index]],[['ratingsList','',index,'img']]]]]]]}}" class="box" onTap="__e"><image mode="aspectFill" src="{{image}}"></image></view></block></view></view></view></view></block></view></view></view>