<view><view class="goods-list"><block a:if="{{goodsList.length==0}}"><view class="empty">购物车空空如也~</view></block><block a:for="{{goodsList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="row"><view data-event-opts="{{[['tap',[['handleSingleDelete',['$0'],[[['goodsList','',index]]]]]]]}}" class="menu" onTap="__e"><view class="icon iconfont"></view></view><view data-event-opts="{{[['touchStart',[['handleTouchStart',[index,'$event']]]],['touchMove',[['handleTouchMove',[index,'$event']]]],['touchEnd',[['handleTouchEnd',[index,'$event']]]]]}}" class="{{(('production')+' '+(theIndex==index?'open':oldIndex==index?'close':''))}}" onTouchStart="__e" onTouchMove="__e" onTouchEnd="__e"><view data-event-opts="{{[['tap',[['handleCheckbox',['$0'],[[['goodsList','',index]]]]]]]}}" class="container" onTap="__e"><view class="checkbox"><view class="{{((item.selected)?'on':'')}}"></view></view></view><view data-event-opts="{{[['tap',[['handleGoodsInfo',['$0'],[[['goodsList','',index]]]]]]]}}" class="goods-info" onTap="__e"><view class="img"><image src="{{item.img}}"></image></view><view class="info"><view class="title">{{item.name}}</view><view class="spec">{{item.spec}}</view><view class="price-number"><view class="price">{{"￥"+item.price}}</view><counter vue-id="{{'63d50225-1-'+index}}" goodsInfo="{{item}}" data-event-opts="{{[['^add',[['add',['$0'],[[['goodsList','',index]]]]]],['^sub',[['sub',['$0'],[[['goodsList','',index]]]]]]]}}" onAdd="__e" onSub="__e" onVueInit="__l"></counter></view></view></view></view></view></block></view><view class="footer" style="{{'bottom:'+(footerbottom)+';'}}"><view data-event-opts="{{[['tap',[['handleSelectedAll',['$event']]]]]}}" class="container" onTap="__e"><view class="checkbox"><view class="{{((isAllSelected)?'on':'')}}"></view></view><view class="text">全选</view></view><block a:if="{{selectedList.length>0}}"><view data-event-opts="{{[['tap',[['handleMulDelete',['$event']]]]]}}" class="delBtn" onTap="__e">删除</view></block><view class="settlement"><view class="sum">合计:<view class="money">{{"￥"+sumPrice}}</view></view><view data-event-opts="{{[['tap',[['handleConfirm',['$event']]]]]}}" class="btn" onTap="__e">{{"结算"+selectedList.length}}</view></view></view></view>